{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react-jsxdev",
    "jsxImportSource": "@emotion/react",
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false
  },
  "include": ["src", "types/*", "node_modules/vite/types/*"],
}
